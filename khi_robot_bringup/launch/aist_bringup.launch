<?xml version="1.0" encoding="utf-8"?>
<launch>

  <arg name="debug"	default="false" />
  <arg name="ip"	default="192.168.0.2" />
  <arg name="period"	default="2" />
  <arg name="sim"	default="false" />

  <include if="$(arg sim)"
	   file="$(find khi_rs_gazebo)/launch/rs007l_world.launch"/>

  <include unless="$(arg sim)"
	   file="$(find khi_robot_bringup)/launch/rs007l_bringup.launch">
    <arg name="debug"		value="$(arg debug)" />
    <arg name="ip"		value="$(arg ip)" />
    <arg name="period"		value="$(arg period)" />
    <arg name="simulation"	value="$(arg sim)" />
  </include>

  <include file="$(find khi_rs007l_moveit_config
		 )/launch/moveit_planning_execution.launch"/>

</launch>
